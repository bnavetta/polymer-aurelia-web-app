---
parser: babel-eslint

env:
  - browser
  - es6

rules:
  comma-dangle: [1, "always-multiline"]
  no-console: 0
  consistent-return: 2
  curly: [2, "multi-line"]
  default-case: 1
  dot-location: [2, "property"]
  eqeqeq: 2
  strict: [2, "global"]
  camelcase: 2
  comma-style: [2, "last"]
  indent: [2, 4, {"indentSwitchCase": true}]
  no-var: 2